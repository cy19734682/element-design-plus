{
  "name": "@yuechan/element-design",
  "version": "2.0.1",
  "description": "A magical vue admin. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "lib",
    "src",
    "types"
  ],
  "main": "./lib/elmDesign.umd.cjs",
  "module": "./lib/elmDesign.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/elmDesign.js",
      "require": "./lib/elmDesign.umd.cjs",
      "types": "./types/index.d.ts"
    },
    "./lib/elmDesign.min.css": "./lib/elmDesign.min.css",
    "./lib/**/*.js": "./lib/**/*.js",
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "clean": "gulp types",
    "types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.app.declaration.json --composite false",
    "node-serve": "node serve/index.js",
    "node-serve-dev": "nodemon serve/index.js",
    "lib": "vue-tsc --noEmit --skipLibCheck && vite build --mode lib && gulp lib"
  },
  "keywords": [
    "vue",
    "vite",
    "typescript",
    "vue-router",
    "pinia",
    "lodash",
    "axios",
    "echarts",
    "moment",
    "wangeditor",
    "xss",
    "js-cookie",
    "ar-cascader",
    "element-plus"
  ],
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/lodash-es": "^4.17.8",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ar-cascader": "^2.0.0",
    "axios": "^1.4.0",
    "deepmerge": "^4.3.1",
    "echarts": "^5.4.2",
    "element-china-area-data": "^6.1.0",
    "element-plus": "^2.4.2",
    "file-saver": "^2.0.5",
    "js-cookie": "3.0.5",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "normalize.css": "^8.0.1",
    "vue": "^3.3.4",
    "vue-baidu-map-3x": "^1.0.35",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/file-saver": "^2.0.7",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "del": "^6.1.1",
    "github-markdown-css": "^5.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.3",
    "pinia-plugin-persist": "^1.0.0",
    "prettier": "^3.0.0",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.6.5"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.5.1",
    "pnpm": ">=8.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cy19734682/element-design-plus.git"
  },
  "author": "cy19734682",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cy19734682/element-design-plus/issues"
  },
  "homepage": "https://github.com/cy19734682/element-design-plus#readme"
}
